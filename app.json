{
  "expo": {
    "name": "att-mobiles",
    "slug": "att-mobiles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/att-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b8df7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raflesngln.attmobiles"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raflesngln.attmobiles"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "463d1347-4e41-4999-bda1-9d7c00fe0176"
      }
    }
  }
}
